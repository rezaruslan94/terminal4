<p id="notice"><%= notice %></p>

<h1>PO UPDATES</h1>
<div class="table-responsive">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>QTY FINISH</th>
        <th>PO ITEM</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @po_updates.each do |po_update| %>
        <tr>
          <td><%= po_update.qty_finish %></td>
          <td><%= po_update.po_item_id %></td>
          <!-- <td><%#= link_to 'Show', po_update %></td> -->
          <td><%= link_to 'EDIT', edit_po_update_path(po_update) %></td>
          <td><%= link_to 'DELETE', po_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br>

<%= link_to 'New Po Update', new_po_update_path %>
