<p id="notice"><%= notice %></p>

<h1>Po Items</h1>

<table>
  <thead>
    <tr>
      <th>Qty</th>
      <th>Po inspect</th>
      <th>Po stuffing</th>
      <th>Po</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @po_items.each do |po_item| %>
      <tr>
        <td><%= po_item.qty %></td>
        <td><%= po_item.po_inspect %></td>
        <td><%= po_item.po_stuffing %></td>
        <td><%= po_item.po_id %></td>
        <td><%= po_item.item_id %></td>
        <td><%= link_to 'Show', po_item %></td>
        <td><%= link_to 'Edit', edit_po_item_path(po_item) %></td>
        <td><%= link_to 'Destroy', po_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Po Item', new_po_item_path %>
